{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../../output",
        "rootDir": "../../"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./duxca.lib.Ajax.ts",
        "./duxca.lib.Canvas.ts",
        "./duxca.lib.FPS.ts",
        "./duxca.lib.Math.ts",
        "./duxca.lib.Metronome.ts",
        "./duxca.lib.OSC.ts",
        "./duxca.lib.RecordBuffer.ts",
        "./duxca.lib.SGSmooth.ts",
        "./duxca.lib.ServerWorker.ts",
        "./duxca.lib.Signal.ts",
        "./duxca.lib.Wave.ts",
        "./duxca.lib.ts"
    ]
}
