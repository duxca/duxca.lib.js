{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../../output",
        "rootDir": "../../"
    },
    "filesGlob": [
        "./**/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./CanvasRender.ts",
        "./Chord.ts",
        "./FPS.ts",
        "./Metronome.ts",
        "./OSC.ts",
        "./RecordBuffer.ts",
        "./Sandbox.ts",
        "./Sandbox2.ts",
        "./Sandbox3.ts",
        "./Sandbox4.ts",
        "./Sandbox5.ts",
        "./Signal.ts",
        "./Statictics.ts",
        "./Util.ts",
        "./Wave.ts"
    ]
}
