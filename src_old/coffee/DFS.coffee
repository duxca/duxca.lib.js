DFS = (tree)->
  recur = (tree, max)->
    head = tree[0]
    tail = tree.slice(1)
    if not Array.isArray(head)
      _max = (if max < head then head else max)
      if tail.length is 0
      then _max
      else recur(tail, _max)
    else recur(tail, recur(head, max))
  recur(tree, 0)
