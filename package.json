{
  "name": "duxca.lib.js",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "npm run update; npm run build",
    "update": "npm update; bower update; tsd update --save --overwrite",
    "build": "npm run build-rm; npm run build-ts; npm run concat",
    "build-rm": "rm -rf output/*; rm -rf lib/*",
    "build-coffee": "node node_modules/coffee-script/bin/coffee --compile --output output/coffee src/coffee/*.coffee",
    "build-ts": "node node_modules/typescript/bin/tsc --target ES5 --noEmitOnError --noImplicitAny --rootDir ./ --outdir output --declaration src/ts/*.ts;",
    "concat": "cd output/src/ts; cat CanvasRender.js Metronome.js OSC.js Sandbox.js Signal.js Statictics.js Util.js > ../../../lib/duxca.lib.js ",
    "report": "open http://localhost:8000/report/",
    "demo": "open http://localhost:8000/demo/",
    "start": "Python -m SimpleHTTPServer 8000 &",
    "stop": "killall -- Python -m SimpleHTTPServer 8000; exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/legokichi/duxca.lib.js.git"
  },
  "author": "legokichi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/legokichi/duxca.lib.js/issues"
  },
  "homepage": "https://github.com/legokichi/duxca.lib.js",
  "devDependencies": {
    "coffee-script": "^1.9.3",
    "espower-cli": "^0.2.0",
    "plato": "^1.5.0",
    "typescript": "^1.5.0-beta"
  },
  "dependencies": {}
}
